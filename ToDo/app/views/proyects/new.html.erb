<% content_for :title do %> New proyect<% end %>

<% form_for (@proyect), :url => proyects_path, :html => { :multipart => true } do |f| %>

  <%= f.error_messages %>
  <%= f.file_field :avatar %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', proyects_path %>